{
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithHttpRequestRawUrl",
      "WithHttpRequestType",
      "WithExceptionDetails"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "$safeprojectname$": "Information",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }$if$ ($useauth$ == True),
  "OAuth2": {
    "Policies": [
      {
        "Policy": "policy",
        "Scopes": [ "scope" ]
      }
    ]
  }$endif$
}
